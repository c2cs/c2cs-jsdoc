{
	"tags"          : {
		"allowUnknownTags" : true,
		"dictionaries"     : [
			"jsdoc",
			"closure"
		]
	},
	"source"        : {
		"include"        : [
			"lib",
			"package.json",
			"README.md"
		],
		"exclude"        : [ ],
		"includePattern" : ".js$",
		"excludePattern" : "(node_modules/|docs)"
	},
	"plugins"       : [
		"plugins/markdown"
	],
	"markdown"      : {
		"excludeTags" : [ "author" ]
	},
	"templates"     : {
		"cleverLinks"    : false,
		"monospaceLinks" : true,
		"addProjectSubdir": false,
		"addVersionSubdir": false
	},
	"opts"          : {
		"destination" : "./doc/html/",
		"encoding"    : "utf8",
		"private"     : true,
		"recurse"     : true,
		"template"    : "./node_modules/c2cs-jsdoc",
		"verbose"     : true
	}
}
